
@model Dental




<form method="post" enctype="multipart/form-data">
	<div class="col-12">
		
		<p>Title</p>
		<input class="form-control mb-3" asp-for="Title" />
		<p>SubTitle</p>
		<input class="form-control mb-3" asp-for="SubTitle" />
		<p>Description</p>
		<input class="form-control mb-3" asp-for="Description" />
		<p>Info</p>
		<input class="form-control mb-3" asp-for="Info" />
		<p>Email</p>
		<input class="form-control mb-3" asp-for="Email" />
		<p>Phone</p>
		<input class="form-control mb-3" asp-for="Phone" />
		<p>Photo</p>
		<input class="form-control mb-2" type="text" name="PhotoURL" />
		<input class="form-control mb-3" type="hidden" asp-for="ID" />
		<input class="form-control mb-3" type="submit" value="Create"/>
		
	</div>
</form>
