@model About




<form method="post" enctype="multipart/form-data">
	<div class="col-12">
		<p>Name</p>
		<input class="form-control mb-3" asp-for="Name" />
		<p>Title</p>
		<input class="form-control mb-3" asp-for="Title" />
		<p>Description</p>
		<input class="form-control mb-3" asp-for="Description" />
		<p>Photo</p>
		<input class="form-control mb-2" type="text" name="PhotoURL" />
		<input class="form-control mb-3" type="hidden" asp-for="ID" />
		<input class="form-control mb-3" type="submit" value="Create"/>
		
	</div>
</form>